#map {
  height: 500px;
  width: 750px;
}

.directory-example {
  height: auto;
  width: 100%;
}

.marker_popup ul {
  list-style: none;
}


/* LEGEND */

/* this part sets it up so that the individual keys will
   automatically float around the page as they are added
   and removed */
.key-container {
  display: flex;
  flex-wrap: wrap;
}

.key {
  flex-basis: 200px;
  flex-grow: 1;
  padding: 1em;
}

.key ul {
  list-style: none;
}

/* individual key entries */
.key li {
  margin-right: 10px;
}

/* TODO possibly use circles since that's what's on map? */
.key span {
  border: 1px solid black;
  height: 10px;
  float: left;
  margin: 2px 5px 2px 2px;
  width: 20px;
}

.key-hidden {
  display: none;
}

.key-icon {
  height: 30px;
  margin: 2px 5px 2px 2px;
  width: 30px;
}

/* directory */
.key .default {
  background-color: #B7B7B7;
}

/* court role key colors */
.key .role-attorney { background-color: red; }
.key .role-clerk { background-color: teal; }
.key .role-defendant { background-color: green; }
.key .role-judge { background-color: purple; }
.key .role-juror { background-color: orange; }
.key .role-pool { background-color: yellow; }
.key .role-jop { background-color: pink; }
.key .role-server { background-color: brown; }
.key .role-witness { background-color: aqua; }
.key .role-mult { background-color: navy; }

/* gender key colors */
.key .gender-f { background-color: #F21DFF; }
.key .gender-m { background-color: cyan; }
.key .gender-u { background-color: orange; }

/* occupation key colors */
.key .occ-admin { background-color: green; }
.key .occ-ag { background-color: red; }
.key .occ-clerical { background-color: #E3EC70; }
.key .occ-prof { background-color: orange; }
.key .occ-sales { background-color: lavender; }
.key .occ-service { background-color: purple; }
.key .occ-skilled { background-color: blue; }
.key .occ-unskilled { background-color: aqua; }

/* race key colors */
.key .race-c { background-color: blue; }
.key .race-w { background-color: yellow; }
