.map_container {
  height: 79vh;
  border: solid 5px #aaa;
  border-right: none;
  border-left: none;
}

.map_container_map {
  height: 100%;
}

#map {
  height: 100%;
  /*margin: 0 auto;*/
}

.map_container_key {
  background: #fff;
}

@media only screen and (min-width: 700px) {

  .map_container {
    display: flex; 
  }

  .map_container_map {
    width: 66vw;
  }

  .map_container_key {
 /*   height: 100%;*/
  }

  .map_container_key > div {
    width: 34vw;
  }

  .tab-content {
    overflow: scroll;
    height: 95%;
    overflow-x: hidden;
  }

  .tab-content ul, .tab-content li {
    margin: 0;
    padding: 0;
  }

  .label-key {
    column-count: 2;
    margin-bottom: 1em !important;
    display: block;
  }

  .label-key li {
    -webkit-column-break-inside: avoid; /* Chrome, Safari */
    page-break-inside: avoid;           /* Theoretically FF 20+ */
    break-inside: avoid-column;         /* IE 11 */
    display:table;                      /* Actually FF 20+ */
  }

  .tab-content {
    font-size: .9em;
    line-height: 1.15em;
  }

  .tab-content h3 {
    margin: 0 0 .5em 0;
    padding: 0;
  }

  .map_container .nav-tabs > li > a {
 
    padding: .4em;
    font-size: .9em;
  }

.map_container_key {
  display: flex;
  flex-direction: column;
}
