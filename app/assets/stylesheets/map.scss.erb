/* LEGEND */

/* this part sets it up so that the individual keys will
   automatically float around the page as they are added
   and removed */
.key-container {
  display: flex;
  flex-wrap: wrap;
}

.key {
  flex-basis: 200px;
  flex-grow: 1;
  padding: 1em;
}

.key ul {
  list-style: none;
}

/* individual key entries */
.key li {
  margin-right: 10px;
}

.label-key li span {
  border-radius:50%;
  border: 1px solid black;
  display: inline-block;
  height: 15px;
  margin: 2px 5px 2px 2px;
  width: 15px;
}

.key-hidden {
  display: none;
}

.key-icon {
  height: 30px;
  margin: 2px 5px 2px 2px;
  width: 30px;
}

/* directory */
.key .default {
  background-color: #B7B7B7;
}

/* court role key colors */
