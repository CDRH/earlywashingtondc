<a name="top">
  <h2>Displaying All <%= @all_docs[:num_found] %> Documents</h2>

  <!-- search form -->
  <%= render "search_bar" %>
  
</a>

<ul>
  <% @all_docs[:docs].each do |doc| %>
    <li>
      <%= link_to "#{doc["title"]} (#{doc["dateNormalized"]})", doc_path(doc["id"]) %>
    </li>
  <% end %>
</ul>