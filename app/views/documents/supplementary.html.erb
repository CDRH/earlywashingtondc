<a name="top">
  <h2>Displaying <%= @docs[:num_found] %> Supplementary Documents</h2>

  <!-- search form -->
  <%= render "search_bar" %>
</a>

<ul>
  <% @docs[:docs].each do |doc| %>
    <li>
      <%= link_to "#{doc["title"]} (#{doc["dateDisplay"]})", doc_path(doc["id"]) %>
    </li>
  <% end %>
</ul>